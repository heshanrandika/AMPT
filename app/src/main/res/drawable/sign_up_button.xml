<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#F6E062" />
            <gradient android:angle="-90" android:startColor="#F6E062" android:endColor="#F6E062"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#F6E062" />
            <solid android:color="#F6E062"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#F9D716" />
            <gradient android:angle="-90" android:startColor="#F9D716" android:endColor="#F9D716" />
        </shape>
    </item>
</selector>